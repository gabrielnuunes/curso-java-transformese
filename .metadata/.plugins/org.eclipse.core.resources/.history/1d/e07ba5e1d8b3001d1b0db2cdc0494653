package controle;

public class IfElse {

	public static void main(String[] args) {
		
		double idade = 20;

		//condição ou verificação
		
		if (idade == 18) {
			System.out.println("Isso é igual a 18!");
		}
		
		if (idade > 18) {
			System.out.println("Isso é maior que 18!");
		}
			
		
		
		
	}
	
}
