package array.exemplo1;

public class RevisaoArray {

	public static void main(String[] args) {
		
		// Criando um array
		int[] numeros = new int[4];
		
		// Atribuir valores ao array
		// numero = [0, 0, 0, 0];
		
		numeros[0] = 1;
		numeros[1] = 3;
		numeros[2] = 6;
		numeros[3] = 5;
		
		// O que eu fiz? 
		// numero = [1, 3, 6, 5];
		
		
		
		
	}
	
}
