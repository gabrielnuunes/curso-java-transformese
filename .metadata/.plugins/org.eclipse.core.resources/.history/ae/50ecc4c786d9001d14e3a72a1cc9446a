package orientacaoobjetos.heranca.atividades.contas;

public class ContaPj extends ContaPf{

	private String nomeEmpresa;
	private String numeroCNPJ;

	public ContaPj(String nomeTitular, int numeroConta, String nomeEmpresa, double saldo , String numeroCNPJ) {
		super(nomeTitular, numeroConta, saldo);
		this.nomeEmpresa = nomeEmpresa;
		this.numeroCNPJ = numeroCNPJ;

	}

	public String getNomeEmpresa() {
		return nomeEmpresa;
	}

	public void setNomeEmpresa(String nomeEmpresa) {
		this.nomeEmpresa = nomeEmpresa;
	}

	public String getNumeroCNPJ() {
		return numeroCNPJ;
	}

	public void setNumeroCNPJ(String numeroCNPJ) {
		this.numeroCNPJ = numeroCNPJ;
	}
		
}
