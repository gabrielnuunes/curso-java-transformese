package introducao;

public class ResolucaoAtividadeUm {

	public static void main(String[] args) {
		
		int numero1 = 40;
		int numeroDois = 30;
		int numero_tres = 70;
		
		double soma = numero1 + 
				numeroDois + numero_tres;
		
		double resultado = soma / 3;
		
		System.out.println(resultado);
				
		
	}
	
}
